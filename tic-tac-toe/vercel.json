{
  "name": "tic-tac-toe-game",
  "version": 2,
  "functions": {
    "app/**/*": {
      "runtime": "nodejs20.x"
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
